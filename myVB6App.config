<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <!--
        This <probing> element tells the .NET runtime to look for assemblies
        in its own directory (the empty privatePath="."). This is a robust
        way to ensure all necessary DLLs are found when the .NET runtime is
        hosted by a native application like a VB6 executable.
      -->
      <probing privatePath="." />
    </assemblyBinding>
  </runtime>
</configuration>